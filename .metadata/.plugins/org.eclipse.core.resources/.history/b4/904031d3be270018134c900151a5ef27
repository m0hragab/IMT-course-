#include"Macros.h"
#include"Std_Types.h"
#include"Dio.h"
#include<avr/delay.h>
#include<avr/interrupt.h>
#include<util/delay.h>
#include"LCD_Interface.h"
#include"TWI_Interface.h"
#include "EEPROM_Interface.h"

int main(void)
{
	u8 arr[]="Mohamed Ragab";
	u8 line[]="Done";
	lcd_vidinit();
	EEPROM_VidInit();
	u8 i=0;
	while(i!='0')
	{
		EEPROM_U8WriteByte(i,arr[i]);
		i++;
	}
	Lcd_vidWriteString(line);

	while (1);
	return 0;
}


