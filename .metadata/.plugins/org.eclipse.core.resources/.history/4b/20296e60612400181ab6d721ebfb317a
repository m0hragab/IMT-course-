int main(void)
{
	lcd_vidinit();
	EEPROM_VidInit();
	u8 receiveddata;
	EEPROM_U8ReadByte(0,&receiveddata);
	lcd_vidwritechar(receiveddata);

	while(1);


	return 0;
}

