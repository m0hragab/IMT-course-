			Dio_vidSetPinValue(Dio_PORTB,2,STD_LOW);
			Dio_vidSetPinValue(Dio_PORTB,3,STD_LOW);
			_delay_ms(2);
			Dio_vidSetPinValue(Dio_PORTB,0,STD_LOW);
			Dio_vidSetPinValue(Dio_PORTB,1,STD_HIGH);
			Dio_vidSetPinValue(Dio_PORTB,2,STD_LOW);
			Dio_vidSetPinValue(Dio_PORTB,3,STD_LOW);
